					<label id="paysNaissanceMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>
				
				<div class="item item-wide" id="villeNaissanceDiv"><!-- Ville de naissance -->	
					<custom-attributes champId="8" />
					<label value="${c:l('Champ.Patient.VilleNaissance')}" sclass="chpLabel formLabel" />
					<div class="row-container changedValue">				
					<label id="villeNaissanceLabelChanged" 
						sclass="modifmultiValue formValue"
						visible="false"
					/>
					<label id="villeNaissanceMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>
				
				<div class="item item-wide" id="patientEtatDiv"><!-- Etat du patient -->	
					<custom-attributes champId="10" />
					<label value="${c:l('Champ.Patient.PatientEtat')}" sclass="chpLabel formLabel" />
					<div class="row-container changedValue">				
					<label id="etatLabelChanged" 
						sclass="modifmultiValue formValue"
						visible="false"
					/>
					<label id="etatMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>
				
				<div class="item item-wide" id="dateEtatDiv"><!-- Date de l'état -->	
					<custom-attributes champId="11" />
					<label value="${c:l('Champ.Patient.DateEtat')}" sclass="chpLabel formLabel" />
					<div class="row-container changedValue">				
					<label id="dateEtatLabelChanged" 
						sclass="modifmultiValue formValue"
						visible="false"
					/>
					<label id="dateEtatMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>																																				

				<div class="item item-wide" id="dateDecesDiv"><!-- Date décès -->	
					<custom-attributes champId="12" />
					<label value="${c:l('Champ.Patient.DateDeces')}" sclass="chpLabel formLabel" />
					<div class="row-container changedValue">				
					<label id="dateDecesLabelChanged" 
						sclass="modifmultiValue formValue"
						visible="false"
					/>
					<label id="dateDecesMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>	
			</div>
		</panelchildren>
		<toolbar height="100%" align="end">
			<hbox spacing="5" align="center">
				<button id="validateModifMultiple" label="${c:l('general.validate')}" height="24px"
					image="/images/icones/save.png" sclass="buttonv2"/>
				<button id="revert" label="${c:l('general.cancel')}" height="24px"
					image="/images/icones/cancel.png" sclass="buttonv2"/>
			</hbox>
		</toolbar>
	</panel>
	
</window>