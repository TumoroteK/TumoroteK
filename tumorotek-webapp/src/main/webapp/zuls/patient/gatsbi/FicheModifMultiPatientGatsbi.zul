						visible="false"
					/>
					<label id="sexeMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>

				<div class="item item-wide" id="dateNaissanceDiv"><!-- Date de naissance -->	
					<custom-attributes champId="7" />
