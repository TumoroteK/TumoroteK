<window id="fwinModifMultiPatient" border="none" apply="fr.aphp.tumorotek.action.patient.gatsbi.FicheModifMultiPatientGatsbi">
	<panel id="winPanel" title="${c:l('fichePatient.panel.title')}"
	 	sclass="fichePanelv2" closable="true">
		<panelchildren id="panelChildrenWithScroll" style="overflow:scroll">
			<div id="gatsbiContainer" class="main-container gridForm1" style="padding-left: 5px; padding-top: 5px">
				<div class="item item-wide" id="nipLabelDiv"><!-- Nip patient -->	
					<custom-attributes champId="2" />
					<label value="${c:l('Champ.Patient.Nip')}" sclass="chpLabel formLabel" />
					<div class="row-container changedValue">				
					<label id="nipLabelChanged" 
						sclass="modifmultiValue formValue"
						visible="false"
					/>
					<label id="nipMultiLabel"
						value="${c:l('general.edit.modification.multiple')}" 
						sclass="formLink" />
					</div>
				</div>
				
				<div class="item item-wide" id="nomDiv"><!-- Nom patient -->	
					<custom-attributes champId="3" />
					<label value="${c:l('Champ.Patient.Nom')}" sclass="chpLabel formLabel" />
					<div class="row-container changedValue">				
					<label id="nomLabelChanged" 
